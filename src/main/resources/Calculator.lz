class Calculator {
    fun fibonacci(num: Int): Int {
        if(num <= 0) {
            return -1
        }

        if(num <= 2) {
            return num - 1
        }

        return fibonacci(num - 1) + fibonacci(num - 2)
    }

    fun factorial(num: Int): Int {
        if(num <= 1) {
            return 1
        }

        return factorial(num - 1) * num
    }

    fun sum(num: Int): Int {
        if(num <= 1) {
            return 1
        }

        return sum(num - 1) + num
    }
}